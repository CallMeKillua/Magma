import { Test, Assertions } from org.junit.jupiter.api;

import {
    { Files, Paths } from nio.file,
    IOException from io,
} from java;

@Test
def test() => {
    const path = Paths.get(".", "src/test/java-gen/com/meti/ApplicationTest.java");
    if (!Files.exists(path)) Files.createDirectories(path.getParent());
    if (!Files.exists(path)) Files.createFile(path);
    Assertions.assertTrue(Files.exists(path));
}