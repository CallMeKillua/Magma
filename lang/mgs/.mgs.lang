<number_type> = "U8" | "I16";
<type> = <number_type>;

<character> = "'" <char> "'";
<number> = [ digit ]("" | <number_type>);

<value> = <character> | <number>;

<declaration> : {
    <flags> = "let" | "const",
    <name> = text,
    <provided_type> = "" | (":" <whitespace> <type>)
} = flags
    <whitespace> <name>
    <whitespace> <provided_type>
    <whitespace> "="
    <whitespace> <value>
    <whitespace> ";";



<statement> = <declaration>;
program = [ <statement> ];